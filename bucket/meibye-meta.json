{
  "version": "0.0.1",
  "description": "Scanner that generates Scoop manifests from installers under C:\\Tools\\apps.",
  "homepage": "https://github.com/meibye/meibye-bucket",
  "license": "MIT",
  "url": "https://raw.githubusercontent.com/meibye/meibye-bucket/master/scripts/apps-bucket-scan.ps1",
  "bin": [
      "apps-bucket-scan.ps1"
  ],
  "notes": [
    "Run 'apps-bucket-scan' to scan C:\\Tools\\apps and write manifests into D:\\Dev\\meibye-bucket\\bucket.",
    "Use -DryRun to dry-run."
  ],
  "checkver": {
    "github": "meibye/meibye-bucket",
    "file": "scripts/apps-bucket-scan.ps1",
    "regex": "scripts/apps-bucket-scan\\.ps1$"  
  },
  "autoupdate": {
    "url": "https://raw.githubusercontent.com/meibye/meibye-bucket/master/scripts/apps-bucket-scan.ps1",
    "hash": { "mode": "download" }
  }
}
